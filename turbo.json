{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "lint": {
      "outputs": []
    },
    "type-check": {
      "outputs": []
    },
    "build": {
      "dependsOn": ["^build", "lint", "type-check"],
      "outputs": [
        "dist/**",
        ".next/**",
        "build/**"
      ]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "test": {
      "dependsOn": ["build"],
      "outputs": ["coverage/**"]
    },
    "start": {
      "dependsOn": ["build"],
      "cache": false
    }
  }
}
